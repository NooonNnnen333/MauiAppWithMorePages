<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Povtorenie333.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Povtorenie333"
    Shell.FlyoutBehavior="Disabled"
    Title="Povtorenie333">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>
        
    <ShellContent
        Title="HouseCreate"
        ContentTemplate="{DataTemplate local:HouseCreatePage}"
        Route="HouseCreate" />

    <ShellContent
        Title="OfficeBuildCreate"
        ContentTemplate="{DataTemplate local:OfficeBuildCreatePage}"
        Route="OfficeBuildCreate"/>
    
    <ShellContent
        Title="DetailObjectPage"
        ContentTemplate="{DataTemplate local:DetailObjectPage}"
        Route="DetailObjectPage"/>
    

</Shell>
